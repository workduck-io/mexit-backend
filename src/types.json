{
    "$id": "types",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "definitions": {
        "AuthorizeRefreshTokenType": {
            "properties": {
                "email": {
                    "type": "string"
                },
                "refreshToken": {
                    "type": "string"
                }
            },
            "required": [
                "email",
                "refreshToken"
            ],
            "type": "object"
        },
        "AuthorizeType": {
            "properties": {
                "email": {
                    "type": "string"
                },
                "password": {
                    "type": "string"
                }
            },
            "required": [
                "email",
                "password"
            ],
            "type": "object"
        },
        "Block": {
            "properties": {
                "elements": {
                    "items": {
                        "$ref": "types#/definitions/NodeChildData"
                    },
                    "type": "array"
                },
                "type": {
                    "type": "string"
                }
            },
            "required": [
                "elements",
                "type"
            ],
            "type": "object"
        },
        "GenericObjectType": {
            "additionalProperties": {
                "$ref": "types#/definitions/GenericType"
            },
            "type": "object"
        },
        "GenericType": {
            "anyOf": [
                {
                    "$ref": "types#/definitions/GenericObjectType"
                },
                {
                    "items": {
                        "$ref": "types#/definitions/GenericType"
                    },
                    "type": "array"
                },
                {
                    "type": [
                        "string",
                        "number",
                        "boolean"
                    ]
                }
            ]
        },
        "NodeChildData": {
            "properties": {
                "children": {
                    "items": {
                        "$ref": "types#/definitions/NodeChildData"
                    },
                    "type": "array"
                },
                "content": {
                    "type": "string"
                },
                "createdBy": {
                    "type": "string"
                },
                "elementType": {
                    "type": "string"
                },
                "id": {
                    "type": "string"
                },
                "properties": {
                    "properties": {
                        "bold": {
                            "type": "boolean"
                        },
                        "italic": {
                            "type": "boolean"
                        }
                    },
                    "required": [
                        "bold",
                        "italic"
                    ],
                    "type": "object"
                }
            },
            "required": [
                "content",
                "elementType",
                "id"
            ],
            "type": "object"
        },
        "NodeData": {
            "properties": {
                "children": {
                    "items": {
                        "$ref": "types#/definitions/NodeChildData"
                    },
                    "type": "array"
                },
                "elementType": {
                    "type": "string"
                },
                "id": {
                    "type": "string"
                }
            },
            "required": [
                "children",
                "elementType",
                "id"
            ],
            "type": "object"
        },
        "NodeDetail": {
            "properties": {
                "data": {
                    "items": {
                        "$ref": "types#/definitions/NodeData"
                    },
                    "type": "array"
                },
                "id": {
                    "type": "string"
                },
                "lastEditedBy": {
                    "type": "string"
                },
                "namespaceIdentifier": {
                    "type": "string"
                },
                "workspaceIdentifier": {
                    "type": "string"
                }
            },
            "required": [
                "data",
                "id",
                "lastEditedBy",
                "namespaceIdentifier",
                "workspaceIdentifier"
            ],
            "type": "object"
        },
        "WDErrorType": {
            "properties": {
                "errorCode": {
                    "$ref": "types#/definitions/errorCodes"
                },
                "errorObject": {},
                "message": {
                    "type": "string"
                },
                "metaData": {
                    "type": "string"
                },
                "statusCode": {
                    "$ref": "types#/definitions/statusCodes"
                }
            },
            "required": [
                "message"
            ],
            "type": "object"
        },
        "errorCodes": {
            "enum": [
                "INPUT_ERROR",
                "NO_CONTENT",
                "UNKNOWN"
            ],
            "type": "string"
        },
        "statusCodes": {
            "enum": [
                200,
                204,
                400,
                401,
                403,
                404,
                408,
                429,
                500,
                502,
                503,
                504
            ],
            "type": "number"
        }
    }
}

