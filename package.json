{
  "name": "mex-browser-extension-backend",
  "version": "1.0.0",
  "main": "app.js",
  "repository": "git@github.com:workduck-io/mex-browser-extension-backend.git",
  "author": "Workduck <humans@workduck.io>",
  "license": "MIT",
  "scripts": {
    "lint": "eslint . --ext .js,.ts,.tsx",
    "lint-fix": "eslint . --ext .js,.ts,.tsx --fix",
    "start": "export NODE_ENV=prod && ts-node src/app.ts",
    "build": "tsc",
    "dev": "export NODE_ENV=dev && nodemon src/app.ts",
    "staging": "export NODE_ENV=staging && nodemon src/app.ts",
    "local": "export NODE_ENV=local && nodemon src/app.ts",
    "debug": "export DEBUG=* && npm run start",
    "test": "echo \"Error: no test specified\" && exit 1",
    "types": "typescript-json-schema \"src/interfaces/*.ts\" \"*\" -o src/types.json --id \"types\" --required"
  },
  "devDependencies": {
    "@types/cors": "^2.8.10",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.13",
    "@types/node": "^8.0.29",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.23.0",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.2",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-node": "^11.1.0",
    "nodemon": "^2.0.15",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "ajv": "7.2.4",
    "amazon-cognito-identity-js": "^5.2.4",
    "axios": "^0.24.0",
    "cors": "^2.8.5",
    "express": "^4.17.2",
    "inversify": "^6.0.1",
    "jsonwebtoken": "^8.5.1",
    "jwk-to-pem": "^2.0.5",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^10.4.0",
    "typescript-json-schema": "^0.52.0"
  }
}
