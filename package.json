{
  "name": "mexit-backend",
  "version": "1.0.0",
  "main": "app.js",
  "repository": "git@github.com:workduck-io/mexit-backend.git",
  "author": "Workduck <humans@workduck.io>",
  "license": "MIT",
  "engines": {
    "node": ">=14.13.1 <=16.14.2"
  },
  "scripts": {
    "lint": "eslint . --ext .js,.ts,.tsx",
    "lint-fix": "eslint . --ext .js,.ts,.tsx --fix",
    "start:prod": "tsc && NODE_ENV=production node dist/app.js",
    "start": "tsc && node dist/app.js",
    "build": "tsc",
    "dev": "nodemon src/app.ts",
    "debug": "export DEBUG=* && npm run start",
    "test-watch": "jest --watchAll --verbose --coverage",
    "test:unit": "jest ./src/__tests__/unit/*",
    "test:integration": "jest ./src/__tests__/integration/*",
    "test": "jest ./src/__tests__/*",
    "types": "typescript-json-schema \"src/interfaces/*.ts\" \"*\" -o src/types.json --id \"types\" --required",
    "docs": "rimraf ./docs && typedoc"
  },
  "devDependencies": {
    "@types/cors": "^2.8.10",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.13",
    "@types/got": "^9.6.12",
    "@types/jest": "^27.4.1",
    "@types/lodash": "^4.14.178",
    "@types/node": "^17.0.25",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.23.0",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.2",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-node": "^11.1.0",
    "jest": "27.4",
    "nodemon": "^2.0.15",
    "ts-jest": "^27.1.3"
  },
  "dependencies": {
    "@aws-sdk/client-cloudwatch-logs": "^3.92.0",
    "ajv": "7.2.4",
    "amazon-cognito-identity-js": "^5.2.4",
    "aws-sdk": "^2.1066.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.2",
    "googleapis": "^97.0.0",
    "got": "11.8.3",
    "inversify": "^6.0.1",
    "jsonwebtoken": "^8.5.1",
    "jwk-to-pem": "^2.0.5",
    "lodash": "^4.17.21",
    "meilisearch": "^0.24.0",
    "mem": "8.1.1",
    "nanoid": "^3.2.0",
    "node-cache": "^5.1.2",
    "reflect-metadata": "^0.1.13",
    "stats-map": "^1.0.0",
    "ts-node": "^10.4.0",
    "typescript": "4.5.4",
    "typescript-json-schema": "^0.52.0",
    "winston": "^3.6.0",
    "winston-cloudwatch": "^6.0.0"
  }
}
